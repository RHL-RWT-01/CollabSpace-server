{
    "name": "collabspace-server",
    "version": "1.0.0",
    "description": "CollabSpace backend server with Express, Socket.IO, MongoDB, and real-time features",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --exec ts-node --project tsconfig.json src/index.ts",
        "build": "tsc && tsc-alias",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "type-check": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "aws-sdk": "^2.1500.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.0",
        "morgan": "^1.10.0",
        "openai": "^4.20.0",
        "razorpay": "^2.9.6",
        "redis": "^4.6.0",
        "resend": "^3.2.0",
        "socket.io": "^4.7.5",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.8.0",
        "@types/redis": "^4.0.11",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    }
}